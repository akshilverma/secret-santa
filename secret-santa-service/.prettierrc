{
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^#src/types/(.*)$",
        "^#src/lib/(.*)$",
        "^#src/controllers/(.*)$",
        "^#src/schemas/(.*)$",
        "^#src/routes/(.*)$",
        "^#src/utils/(.*)$",
        "^[./]"
    ],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
